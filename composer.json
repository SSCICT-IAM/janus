{
    "name":"janus-ssp/janus",
    "description":"SimpleSaml php Janus module",
    "minimum-stability": "stable",
    "require":{
        "php": ">=5.3.3",
        "symfony/symfony": "~2.3",
        "symfony/icu": "1.0.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "1.2.*",
        "doctrine/migrations": "dev-master as 1",
        "doctrine/doctrine-migrations-bundle": "dev-master as 1",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "~2.3",
        "symfony/yaml" : "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "components/jquery":"1.9.1",
        "components/jqueryui":"1.10.2",
        "pimple/pimple":"1.0.x",
        "jms/serializer": "0.13.0",
        "jms/serializer-bundle": "0.12",
        "jms/security-extra-bundle": "~1.5.1",
        "simplesamlphp/simplesamlphp": "*",
        "friendsofsymfony/rest-bundle": "~1.1",
        "nelmio/api-doc-bundle": "~2.5.0",
        "fsc/hateoas-bundle": "dev-master#e36875985202c91eb52d4cc2923766b0b015539a as 0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phake/phake": "1.*",
        "nelmio/alice": "1.6.0",
        "guzzle/guzzle": "3.8.1"
    },
    "autoload": {
        "classmap": [ "src/", "lib/", "tests/"]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    }
}
